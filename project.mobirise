        {
          "alias": false,
          "_styles": {
            "& when not (@fullScreen)": {
              "padding-top": "(@paddingTop * 15px)",
              "padding-bottom": "(@paddingBottom * 15px)"
            },
            "& when (@bg-type = \"color\")": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = \"image\")": {
              "background-image": "url(@bg-value)"
            },
            "& when (@cardsAmount > 3)": {
              ".card-wrapper .card-img .meta-category": {
                "top": "165px"
              }
            },
            ".contents": {
              "text-align": "center",
              "margin-bottom": "54px",
              ".mbr-section-title": {
                "margin-bottom": "20px",
                "color": "@primaryColor"
              },
              ".mbr-subtitle": {
                "color": "@secondaryColor",
                "margin-bottom": "10px"
              },
              ".mbr-section-text": {
                "margin-top": "20px",
                "color": "@infoColor"
              }
            },
            ".card-wrapper": {
              "height": "100%",
              "margin-top": "15px",
              "margin-bottom": "15px",
              ".card-box": {
                "padding": "35px 30px 0px",
                ".card-title": {
                  "color": "@primaryColor",
                  "transition": "0.3s all",
                  "margin-bottom": "20px",
                  "&:hover": {
                    "color": "@primaryColor",
                    "transition": "0.3s all"
                  }
                },
                ".card-subtitle": {
                  "color": "@primaryColor",
                  "margin-bottom": "13px",
                  "font-weight": "500",
                  "span": {
                    "color": "@partOfDateColor"
                  }
                },
                ".mbr-text": {
                  "margin-bottom": "25px",
                  "color": "@infoColor"
                },
                ".mbr-section-btn": {
                  "position": "relative",
                  "margin-left": "1px",
                  ".link": {
                    "display": "flex !important",
                    "justify-content": "flex-start",
                    "align-items": "center"
                  },
                  ".mbr-iconfont": {
                    "font-size": "16px",
                    "margin-right": "40px",
                    "position": "relative",
                    "top": "1px",
                    "display": "inline-flex",
                    "width": "46px",
                    "height": "46px",
                    "background": "0 0",
                    "color": "@iconColor",
                    "align-items": "center",
                    "justify-content": "center",
                    "transition": "all .3s linear 0s",
                    "order": "2",
                    "&::after": {
                      "content": "\"\"",
                      "position": "absolute",
                      "top": "0",
                      "left": "0",
                      "display": "block",
                      "width": "100%",
                      "height": "100%",
                      "background-color": "@iconBgColorHover",
                      "transform": "scale(0)",
                      "transition": "all .3s linear 0s"
                    },
                    "&::before": {
                      "position": "relative",
                      "z-index": "1"
                    }
                  }
                },
                "&:hover": {
                  ".mbr-section-btn": {
                    ".mbr-iconfont": {
                      "&::before": {
                        "color": "@iconColorHover"
                      },
                      "&::after": {
                        "transform": "scale(1)"
                      }
                    }
                  }
                }
              },
              ".card-img": {
                "position": "relative",
                "overflow": "hidden",
                "img": {
                  "border": "none",
                  "border-radius": "0",
                  "box-shadow": "none",
                  "box-sizing": "border-box",
                  "vertical-align": "middle",
                  "transition": "all .8s",
                  "opacity": "1",
                  "width": "100%",
                  "max-width": "100%",
                  "margin": "auto",
                  "height": "auto",
                  "display": "block"
                },
                "&:hover": {
                  "img": {
                    "transform": "scale3d(1.05, 1.05, 1)",
                    "opacity": ".9"
                  }
                }
              }
            },
            "@media (max-width: 768px)": {
              ".card-wrapper .card-box .mbr-section-btn .link": {
                "justify-content": "center"
              }
            },
            ".card-wrapper .card-box .mbr-text, .mbr-section-btn, .mbr-section-btn": {
              "text-align": "right"
            },
            ".card-wrapper .card-box .card-title, .mbr-section-btn": {
              "text-align": "right"
            }
          },
          "_name": "features2",
          "_sourceTheme": "cleanm4",
          "_customHTML": "<section class=\"features2\" group=\"Features\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax, 'mbr-fullscreen': fullScreen}\">\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"9\" step=\"1\" value=\"4\" condition=\"!fullScreen\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"9\" step=\"1\" value=\"4\" condition=\"!fullScreen\">\n        <header>Show / Hide</header>\n        <input type=\"checkbox\" title=\"Main Title\" name=\"showMainTitle\" checked>\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\" checked>\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\">\n        <input type=\"checkbox\" title=\"Card Title\" name=\"showCardTitle\">\n        <input type=\"checkbox\" title=\"Card Subtitle\" name=\"showCardSubtitle\">\n        <input type=\"checkbox\" title=\"Card Text\" name=\"showCardText\">\n        <input type=\"checkbox\" title=\"Card Link\" name=\"showLink\">\n        <select title=\"Cards\" name=\"cardsAmount\">\n            <option value=\"1\" selected>1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\">4</option>\n            <option value=\"5\">5</option>\n            <option value=\"6\">6</option>\n        </select>\n        <header>Colors</header>\n        <input type=\"color\" name=\"borderColor\" title=\"Borders\" value=\"#dcdfe2\" selected>\n        <input type=\"color\" name=\"partOfDateColor\" title=\"Date\" value=\"#83868c\" selected>\n        <input type=\"color\" name=\"iconColor\" title=\"Icons\" value=\"#14287f\" selected>\n        <input type=\"color\" name=\"iconColorHover\" title=\"Icons Hover\" value=\"#ffffff\" selected>\n        <input type=\"color\" name=\"iconBgColorHover\" title=\"Icons Hover Bg\" value=\"#14287f\" selected>\n\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Background Image\" value=\"@PROJECT_PATH@/assets/images/background-1623x1074.png\" selected>\n            <input type=\"color\" title=\"Background Color\" value=\"#ffffff\">\n            <input type=\"video\" title=\"Background Video\" value=\"https://www.youtube.com/watch?v=36YgDDJ7XSc\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" checked condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay && bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div mbr-class=\"{'container-fluid': fullWidth, 'container': !fullWidth }\">\n        <div class=\"contents\">\n\n            <h1 class=\"mbr-section-title mbr-fonts-style mbr-bold\" data-app-selector=\".contents .mbr-section-title\" mbr-if=\"showMainTitle\" mbr-theme-style=\"display-2\"><span style=\"font-weight: normal;\">Our Branch</span></h1>\n            <h3 class=\"mbr-subtitle mbr-fonts-style mbr-semibold\" data-app-selector=\".contents .mbr-subtitle\" mbr-if=\"showSubtitle\" mbr-theme-style=\"display-5\"><b>Riyadh</b>  Al Khobar  Hail  Jeddah  Yanbu</h3>\n            <h2 class=\"mbr-section-text mbr-fonts-style mbr-regular\" data-app-selector=\".contents .mbr-section-text\" mbr-if=\"showText\" mbr-theme-style=\"display-4\">Condimentum vitae sapien pellentesque habitant morbi\n                tristique senectus.\n                Nibh cras pulvinar mattis nunc.</h2>\n        </div>\n        <div class=\"row justify-content-center\">\n            <div class=\"card col-10 col-md-10\" mbr-class=\"{'col-lg-4': cardsAmount == '12' || cardsAmount == '5' || cardsAmount == '12', 'col-lg-12': cardsAmount == '12'}\">\n                <div class=\"card-wrapper\">\n                    <div class=\"card-img\">\n                        <img src=\"@PROJECT_PATH@/assets/images/maps.svg\" alt=\"Mobirise\">\n                    </div>\n                    <div class=\"card-box align-left\">\n                        <h5 class=\"card-subtitle align-left mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showCardSubtitle\" data-app-selector=\".card-wrapper .card-box .card-subtitle, .mbr-section-btn\">Office Cleaning\n                            <span>/ July 10, 2025</span></h5>\n                        <h4 class=\"mbr-fonts-style mbr-semibold align-left card-title\" mbr-theme-style=\"display-7\" mbr-if=\"showCardTitle\" data-app-selector=\".card-wrapper .card-box .card-title, .mbr-section-btn\">Jack Hammer</h4>\n                        <p class=\"mbr-fonts-style mbr-text mbr-regular align-left\" mbr-theme-style=\"display-4\" mbr-if=\"showCardText\" data-app-selector=\".card-wrapper .card-box .mbr-text, .mbr-section-btn\">Air leg rock Drill:&nbsp;YT24/YT26/YT27 </p>\n\n                        <div class=\"mbr-section-btn mbr-fonts-style mbr-text\" mbr-buttons=\"true\" mbr-theme-style=\"display-4\" data-toolbar=\"mbrBtnMove,mbrBtnLink,mbrBtnAdd\" data-app-selector=\".mbr-fonts-style\" mbr-if=\"showLink\">\n                            <a class=\"mbr-section-text mbr-bold pb-3 mbr-fonts-style d-inline-block link text-primary\" mbr-if=\"showLink\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-fonts-style\">\n                                Read More<span mbr-icon class=\"mbrib-plus mbr-iconfont mbr-iconfont-btn mbr-bold\"></span></a>\n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div class=\"card col-12 col-md-6\" mbr-if=\"cardsAmount > 1\" mbr-class=\"{'col-lg-6': cardsAmount == '4',\n                'col-lg-4': cardsAmount == '3' || cardsAmount == '5' || cardsAmount == '6'}\">\n                <div class=\"card-wrapper\">\n                    <div class=\"card-img\">\n                        <img src=\"@PROJECT_PATH@/assets/images/asset%203@2x-80-700x700.jpg\" alt=\"Mobirise\">\n                    </div>\n                    <div class=\"card-box align-left\">\n                        <h5 class=\"card-subtitle align-left mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showCardSubtitle\" data-app-selector=\".card-wrapper .card-box .card-subtitle, .mbr-section-btn\">House Cleaning\n                            <span>/ July 15, 2025</span></h5>\n                        <h4 class=\"mbr-fonts-style mbr-semibold align-left card-title\" mbr-theme-style=\"display-7\" mbr-if=\"showCardTitle\" data-app-selector=\".card-wrapper .card-box .card-title, .mbr-section-btn\">DTH Hammer</h4>\n                        <p class=\"mbr-fonts-style mbr-text mbr-regular align-left\" mbr-theme-style=\"display-4\" mbr-if=\"showCardText\" data-app-selector=\".card-wrapper .card-box .mbr-text, .mbr-section-btn\">all Types</p>\n\n                        <div class=\"mbr-section-btn mbr-fonts-style mbr-text\" mbr-buttons=\"true\" mbr-theme-style=\"display-4\" data-toolbar=\"mbrBtnMove,mbrBtnLink,mbrBtnAdd\" data-app-selector=\".mbr-fonts-style\" mbr-if=\"showLink\">\n                            <a class=\"mbr-section-text mbr-bold pb-3 mbr-fonts-style d-inline-block link text-primary\" mbr-if=\"showLink\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-fonts-style\">\n                                Read More<span mbr-icon class=\"mbrib-plus mbr-iconfont mbr-iconfont-btn mbr-bold\"></span></a>\n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div class=\"card col-12 col-md-6\" mbr-if=\"cardsAmount > 2\" mbr-class=\"{'col-lg-6': cardsAmount == '4',\n                'col-lg-4': cardsAmount == '3' || cardsAmount == '5'|| cardsAmount == '6'}\">\n                <div class=\"card-wrapper\">\n                    <div class=\"card-img\">\n                        <img src=\"@PROJECT_PATH@/assets/images/asset%204@2x-80-700x700.jpg\" alt=\"Mobirise\">\n                    </div>\n                    <div class=\"card-box align-left\">\n                        <h5 class=\"card-subtitle align-left mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showCardSubtitle\" data-app-selector=\".card-wrapper .card-box .card-subtitle, .mbr-section-btn\">Carpet Cleaning\n                            <span>/ July 20, 2025</span></h5>\n                        <h4 class=\"mbr-fonts-style mbr-semibold align-left card-title\" mbr-theme-style=\"display-7\" mbr-if=\"showCardTitle\" data-app-selector=\".card-wrapper .card-box .card-title, .mbr-section-btn\">Threaded drill bits</h4>\n                        <p class=\"mbr-fonts-style mbr-text mbr-regular align-left\" mbr-theme-style=\"display-4\" mbr-if=\"showCardText\" data-app-selector=\".card-wrapper .card-box .mbr-text, .mbr-section-btn\">/R25/R28/R32/T38/T45/T51/T60</p>\n\n                        <div class=\"mbr-section-btn mbr-fonts-style mbr-text\" mbr-buttons=\"true\" mbr-theme-style=\"display-4\" data-toolbar=\"mbrBtnMove,mbrBtnLink,mbrBtnAdd\" data-app-selector=\".mbr-fonts-style\" mbr-if=\"showLink\">\n                            <a class=\"mbr-section-text mbr-bold pb-3 mbr-fonts-style d-inline-block link text-primary\" mbr-if=\"showLink\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-fonts-style\">\n                                Read More<span mbr-icon class=\"mbrib-plus mbr-iconfont mbr-iconfont-btn mbr-bold\"></span></a>\n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div class=\"card col-12 col-md-6\" mbr-if=\"cardsAmount > 3\" mbr-class=\"{'col-lg-6': cardsAmount == '4',\n                'col-lg-4': cardsAmount == '3' || cardsAmount == '6'}\">\n                <div class=\"card-wrapper\">\n                    <div class=\"card-img\">\n                        <img src=\"@PROJECT_PATH@/assets/images/mbr-700x463.jpeg\" alt=\"Mobirise\">\n                    </div>\n                    <div class=\"card-box align-left\">\n                        <h5 class=\"card-subtitle align-left mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showCardSubtitle\" data-app-selector=\".card-wrapper .card-box .card-subtitle, .mbr-section-btn\">Office Cleaning\n                            <span>/ July 25, 2025</span></h5>\n                        <h4 class=\"mbr-fonts-style mbr-semibold align-left card-title\" mbr-theme-style=\"display-7\" mbr-if=\"showCardTitle\" data-app-selector=\".card-wrapper .card-box .card-title, .mbr-section-btn\">Testing Service</h4>\n                        <p class=\"mbr-fonts-style mbr-text mbr-regular align-left\" mbr-theme-style=\"display-4\" mbr-if=\"showCardText\" data-app-selector=\".card-wrapper .card-box .mbr-text, .mbr-section-btn, .mbr-section-btn\">\n                            In an effort to reduce technical barriers to trade and to facilitate the free movement of goods between the countries of the Gulf Cooperation Council (GCC)</p>\n\n                        <div class=\"mbr-section-btn mbr-fonts-style mbr-text\" mbr-buttons=\"true\" mbr-theme-style=\"display-4\" data-toolbar=\"mbrBtnMove,mbrBtnLink,mbrBtnAdd\" data-app-selector=\".mbr-fonts-style\" mbr-if=\"showLink\">\n                            <a class=\"mbr-section-text mbr-bold pb-3 mbr-fonts-style d-inline-block link text-primary\" mbr-if=\"showLink\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-fonts-style\">\n                                Read More<span mbr-icon class=\"mbrib-plus mbr-iconfont mbr-iconfont-btn mbr-bold\"></span></a>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"card col-12 col-md-6\" mbr-if=\"cardsAmount > 4\" mbr-class=\"{'col-lg-6': cardsAmount == '4',\n                'col-lg-4': cardsAmount == '3' || cardsAmount == '6'}\">\n                <div class=\"card-wrapper\">\n                    <div class=\"card-img\">\n                        <img src=\"@PROJECT_PATH@/assets/images/unicoil-saso-quality-mark-700x990.jpeg\" alt=\"Mobirise\">\n                    </div>\n                    <div class=\"card-box align-left\">\n                        <h5 class=\"card-subtitle align-left mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showCardSubtitle\" data-app-selector=\".card-wrapper .card-box .card-subtitle, .mbr-section-btn\">Carpet Cleaning\n                            <span>/ July 30, 2025</span></h5>\n                        <h4 class=\"mbr-fonts-style mbr-semibold align-left card-title\" mbr-theme-style=\"display-7\" mbr-if=\"showCardTitle\" data-app-selector=\".card-wrapper .card-box .card-title, .mbr-section-btn\">Saudi Quality Mark (SQM)</h4>\n                        <p class=\"mbr-fonts-style mbr-text mbr-regular align-left\" mbr-theme-style=\"display-4\" mbr-if=\"showCardText\" data-app-selector=\".card-wrapper .card-box .mbr-text, .mbr-section-btn, .mbr-section-btn\">\n                            Cras\n                            tempus consectetur venenatis.\n                            Quisque\n                            porttitor iaculis rutrum. Proin sed massa euismod, viverra lacus a, pellentesque nisi.\n                            Integer rutrum mi non est blandit rhoncus. Ut id enim est.</p>\n\n                        <div class=\"mbr-section-btn mbr-fonts-style mbr-text\" mbr-buttons=\"true\" mbr-theme-style=\"display-4\" data-toolbar=\"mbrBtnMove,mbrBtnLink,mbrBtnAdd\" data-app-selector=\".mbr-fonts-style\" mbr-if=\"showLink\">\n                            <a class=\"mbr-section-text mbr-bold pb-3 mbr-fonts-style d-inline-block link text-primary\" mbr-if=\"showLink\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-fonts-style\">\n                                Read More<span mbr-icon class=\"mbrib-plus mbr-iconfont mbr-iconfont-btn mbr-bold\"></span></a>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"card col-12 col-md-6\" mbr-if=\"cardsAmount > 5\" mbr-class=\"{'col-lg-6': cardsAmount == '4',\n                'col-lg-4': cardsAmount == '3' || cardsAmount == '6'}\">\n                <div class=\"card-wrapper\">\n                    <div class=\"card-img\">\n                        <img src=\"@PROJECT_PATH@/assets/images/dd33wneuqaa-pud-700x1188.jpeg\" alt=\"Mobirise\">\n                    </div>\n                    <div class=\"card-box align-left\">\n                        <h5 class=\"card-subtitle align-left mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showCardSubtitle\" data-app-selector=\".card-wrapper .card-box .card-subtitle, .mbr-section-btn\">Toilet Cleaning\n                            <span>/ June 10, 2025</span></h5>\n                        <h4 class=\"mbr-fonts-style mbr-semibold align-left card-title\" mbr-theme-style=\"display-7\" mbr-if=\"showCardTitle\" data-app-selector=\".card-wrapper .card-box .card-title, .mbr-section-btn\">SASO IECEE Recognition Certificate&nbsp;</h4>\n                        <p class=\"mbr-fonts-style mbr-text mbr-regular align-left\" mbr-theme-style=\"display-4\" mbr-if=\"showCardText\" data-app-selector=\".card-wrapper .card-box .mbr-text, .mbr-section-btn, .mbr-section-btn\">\n                            The Saudi Organization for Standardization, Metrology and Quality (SASO) grants a certificate of recognition of the international certificate&nbsp;</p>\n\n                        <div class=\"mbr-section-btn mbr-fonts-style mbr-text\" mbr-buttons=\"true\" mbr-theme-style=\"display-4\" data-toolbar=\"mbrBtnMove,mbrBtnLink,mbrBtnAdd\" data-app-selector=\".mbr-fonts-style\" mbr-if=\"showLink\">\n                            <a class=\"mbr-section-text mbr-bold pb-3 mbr-fonts-style d-inline-block link text-primary\" mbr-if=\"showLink\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-fonts-style\">\n                                Read More<span mbr-icon class=\"mbrib-plus mbr-iconfont mbr-iconfont-btn mbr-bold\"></span></a>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
